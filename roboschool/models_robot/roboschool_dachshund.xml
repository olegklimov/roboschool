<mujoco model="roboschool_dachshund">
  <compiler angle="degree" coordinate="global" inertiafromgeom="true"/>
  <default>
    <joint armature="1" damping="1" limited="true"/>
    <geom conaffinity="1" condim="1" contype="1" margin="0.001" friction="0.8 .1 .1" material="geom" rgba="0.8 0.6 .4 1" solimp=".8 .8 .01" solref=".02 1"/>
    <motor ctrllimited="true" ctrlrange="-.4 .4"/>
  </default>
  <option integrator="RK4" timestep="0.002"/>
  <worldbody>
    <body name="body1">
      <body name="body2">
        <body name="body3">
          <joint axis="0 1 0" name="body23vert" pos="-0.20 0 0" range="-10 10" type="hinge"/>
          <joint axis="0 0 1" name="body23side" pos="-0.20 0 0" range="-20 20" type="hinge"/>
          <geom fromto="-0.24 0 0  -0.36 0 0" name="body3_geom" size="0.08" type="capsule"/>

          <body name="tail">
            <joint axis="0 0 1" name="tail_wag"  pos="-0.44 0 +0.01" range="-45 45" type="hinge"/>
            <joint axis="0 1 0" name="tail_lift" pos="-0.44 0 +0.01" range="10 110" type="hinge"/>
            <geom fromto="-0.44 0 +0.01   -0.44 0 -0.12" name="tail" size="0.02" type="capsule" rgba=".8 .4 .3 1"/>
          </body>

          <!-- back legs -->
          <body name="rr_leg_thigh">
            <joint axis="-1 0 0" name="rr_leg_thigh_rock" pos="-0.38 -0.101 -0.01" range="-30 30" type="hinge"/>
            <joint axis="0 -1 0" name="rr_leg_thigh_vert" pos="-0.38 -0.101 -0.01" range="-50 90" type="hinge"/>
            <geom fromto="-0.38 -0.101 -0.01     -0.38 -0.101 -0.12" name="rr_leg_thigh" size="0.05" type="capsule"/>
            <body name="rr_leg_hock">
               <joint axis="0 -1 0" name="rr_leg_hock_vert" pos="-0.38 -0.101 -0.16" range="-130 0" type="hinge"/>
               <geom fromto="-0.38 -0.101 -0.16  -0.38 -0.101 -0.26" name="rr_leg_hock" size="0.030" type="capsule"/>
               <body name="rr_leg_hind">
                 <joint axis="0 -1 0" name="rr_leg_hind_vert" pos="-0.38 -0.101 -0.29" range="-10 130" type="hinge"/>
                 <geom fromto="-0.38 -0.101 -0.30  -0.38 -0.101 -0.36" name="rr_leg_hind" size="0.030" type="capsule"/>
                 <geom pos="-0.38 -0.101 -0.36" name="rr_leg_foot" size="0.035" type="sphere"/>
               </body>
            </body>
          </body>
          <body name="lr_leg_thigh">
            <joint axis="-1 0 0" name="lr_leg_thigh_rock" pos="-0.38 +0.101 -0.01" range="-30 30" type="hinge"/>
            <joint axis="0 -1 0" name="lr_leg_thigh_vert" pos="-0.38 +0.101 -0.01" range="-50 90" type="hinge"/>
            <geom fromto="-0.38 +0.101 -0.01     -0.38 +0.101 -0.12" name="lr_leg_thigh" size="0.05" type="capsule"/>
            <body name="lr_leg_hock">
               <joint axis="0 -1 0" name="lr_leg_hock_vert" pos="-0.38 +0.101 -0.16" range="-130 0" type="hinge"/>
               <geom fromto="-0.38 +0.101 -0.16  -0.38 +0.101 -0.26" name="lr_leg_hock" size="0.030" type="capsule"/>
               <body name="lr_leg_hind">
                 <joint axis="0 -1 0" name="lr_leg_hind_vert" pos="-0.38 +0.101 -0.29" range="-10 130" type="hinge"/>
                 <geom fromto="-0.38 +0.101 -0.30  -0.38 +0.101 -0.36" name="lr_leg_hind" size="0.030" type="capsule"/>
                 <geom pos="-0.38 +0.101 -0.36" name="lr_leg_foot" size="0.035" type="sphere"/>
               </body>
            </body>
          </body>

        </body>
        <joint axis="0 1 0" name="body12vert" pos="-0.00 0 0" range="-10 10" type="hinge"/>
        <joint axis="0 0 1" name="body12side" pos="-0.00 0 0" range="-20 20" type="hinge"/>
        <geom fromto="-0.04 0 0  -0.16 0 0" name="body2_geom" size="0.09" type="capsule"/>
      </body>
      <geom fromto="+0.04 0 0  +0.16 0 0" name="body1_geom" size="0.09" type="capsule"/>

      <!-- front legs -->
      <body name="rf_leg_shoulder">
        <joint axis="-1 0 0" name="rf_leg_shoulder_rock" pos="0.2 -0.08 -0.01" range="-30 30" type="hinge"/>
        <joint axis="0 -1 0" name="rf_leg_shoulder_vert" pos="0.2 -0.08 -0.01" range="-50 90" type="hinge"/>
        <geom fromto="+0.20 -0.08 -0.01     +0.20 -0.08 -0.12" name="rf_leg_shoulder" size="0.04" type="capsule"/>
        <body name="rf_leg_elbow">
           <joint axis="0 -1 0" name="rf_leg_elbow_vert" pos="0.2 -0.08 -0.16" range="0 130" type="hinge"/>
           <geom fromto="+0.20 -0.08 -0.16  +0.20 -0.08 -0.26" name="rf_leg_elbow" size="0.030" type="capsule"/>
           <body name="rf_leg_forefoot">
             <joint axis="0 -1 0" name="rf_leg_forefoot_vert" pos="0.2 -0.08 -0.29" range="-120 90" type="hinge"/>
             <geom fromto="+0.20 -0.08 -0.30  +0.20 -0.08 -0.34" name="rf_leg_forefoot" size="0.030" type="capsule"/>
             <geom pos="+0.20 -0.08 -0.34" name="rf_leg_foot" size="0.035" type="sphere"/>
           </body>
        </body>
      </body>
      <body name="lf_leg_shoulder">
        <joint axis="-1 0 0" name="lf_leg_shoulder_rock" pos="0.2 +0.08 -0.01" range="-30 30" type="hinge"/>
        <joint axis="0 -1 0" name="lf_leg_shoulder_vert" pos="0.2 +0.08 -0.01" range="-50 90" type="hinge"/>
        <geom fromto="+0.20 +0.08 -0.01     +0.20 +0.08 -0.12" name="lf_leg_shoulder" size="0.04" type="capsule"/>
        <body name="lf_leg_elbow">
           <joint axis="0 -1 0" name="lf_leg_elbow_vert" pos="0.2 +0.08 -0.16" range="0 130" type="hinge"/>
           <geom fromto="+0.20 +0.08 -0.16  +0.20 +0.08 -0.26" name="lf_leg_elbow" size="0.030" type="capsule"/>
           <body name="lf_leg_forefoot">
             <joint axis="0 -1 0" name="lf_leg_forefoot_vert" pos="0.2 +0.08 -0.29" range="-120 90" type="hinge"/>
             <geom fromto="+0.20 +0.08 -0.30  +0.20 +0.08 -0.34" name="lf_leg_forefoot" size="0.030" type="capsule"/>
             <geom pos="+0.20 +0.08 -0.34" name="lf_leg_foot" size="0.035" type="sphere"/>
           </body>
        </body>
      </body>

      <!-- head -->
      <body name="neck">
        <joint axis="0 1 0" name="bodyneck_vert" pos="0.18 0 0.05" range="-30 90" type="hinge"/>
        <joint axis="0 0 1" name="bodyneck_side" pos="0.18 0 0.05" range="-30 30" type="hinge"/>
        <geom fromto="0.22 0 0.12  0.3 0 0.2" name="neck_geom" size="0.05" type="capsule"/>
        <body name="head">
          <joint axis="0 1 0" name="neckhead_vert" pos="0.3 0 0.2" range="-60 30" type="hinge"/>
          <joint axis="0 0 1" name="neckhead_side" pos="0.3 0 0.2" range="-110 110" type="hinge"/>
          <geom name="head" pos="0.30 0 0.24" size=".10" type="sphere"/>
          <geom name="ear1" fromto="0.27 -0.09 0.25  0.27 -0.09 0.18" size=".05" type="capsule" rgba=".8 .4 .3 1"/>
          <geom name="ear2" fromto="0.27 +0.09 0.25  0.27 +0.09 0.18" size=".05" type="capsule" rgba=".8 .4 .3 1"/>
          <geom name="eye1" pos="0.375 +0.035 0.28" size=".019" type="sphere" rgba=".1 .1 .1 1"/>
          <geom name="eye2" pos="0.375 -0.035 0.28" size=".019" type="sphere" rgba=".1 .1 .1 1"/>
          <geom fromto="0.3 0 0.22  0.43 0 0.22" name="headlong" size="0.050" type="capsule"/>
          <geom name="nose" pos="0.45 0 0.26" size=".027" type="sphere" rgba=".2 .2 .2 1"/>
          <geom name="mouth1" pos="0.38 0 0.189" size=".15 .05 .035" type="box"/>
          <geom name="mouth2" pos="0.38 0 0.159" size=".15 .05 .010" type="box"/>
        </body>
      </body>
    </body>
  </worldbody>
</mujoco>
